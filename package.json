    "postcss": "^8.4.47",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.3",
    "vite-plugin-remove-console": "^2.2.0",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.3.0",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "overrides": {
      "vite-plugin-checker": "^0.8.0",
      "vue-tsc": "2.0.29",
      "drizzle-orm": "^0.33.0"
    }
  }
