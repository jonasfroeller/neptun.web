{
  "name": "neptun.web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "app": "export EDITOR=code && nuxt dev",
    "dev": "set EDITOR=code && nuxt dev",
    "devx": "set EDITOR=code && nuxt dev --host --https --ssl-cert ./configurations/development/ssl/server.crt --ssl-key ./configurations/development/ssl/server.key",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "npx prettier --plugin-search-dir . --check . && npx eslint .",
    "format": "npx prettier --write . --single-quote --trailing-comma es5 --semi",
    "ui": "npx shadcn-vue@latest add",
    "cleanup": "pnpx nuxi cleanup .",
    "fresh": "npm cache verify && pnpm rebuild && pnpx nuxi cleanup",
    "check": "npx nuxi typecheck",
    "types": "npx nuxi prepare",
    "upgrade": "npx nuxi upgrade -f",
    "info": "npx nuxi info",
    "find-version": "node ./info/find-package-version.cjs",
    "statistics": "npx nuxi analyze",
    "db:push": "drizzle-kit push --config drizzle.config.ts",
    "db:pull": "drizzle-kit introspect --config drizzle.config.ts",
    "db:generate": "drizzle-kit generate --dialect postgresql --schema ./lib/types/database.tables/schema.ts --out=./server/database/migrations",
    "db:migrate": "bun run ./server/utils/migrate.ts",
    "db:studio": "drizzle-kit studio --config drizzle.config.ts --host 127.0.0.1",
    "db:dump-data": "bun run ./server/utils/backup.ts",
    "db:dump-schema": "bun run ./server/utils/backup.ts --schema-only",
    "db:mermaid": "bun run ./server/utils/schema-to-mermaid.ts",
    "db:png": "bun run ./server/utils/mermaid-to-png.ts",
    "db:diagram": "bun run db:mermaid && bun run db:png"
  },
  "dependencies": {
    "@ai-sdk/vue": "^0.0.58",
    "@formkit/auto-animate": "^0.8.2",
    "@huggingface/inference": "^2.8.1",
    "@neondatabase/serverless": "^0.10.1",
    "@nuxt/image": "^1.8.1",
    "@nuxtjs/device": "^3.2.4",
    "@nuxtjs/mdc": "^0.9.2",
    "@nuxtjs/robots": "^4.1.10",
    "@radix-icons/vue": "^1.0.0",
    "@tanstack/vue-table": "^8.20.5",
    "@vee-validate/zod": "^4.14.6",
    "@vercel/kv": "^1.0.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue-email/components": "^0.0.19",
    "@vue-email/render": "^0.0.9",
    "ai": "^3.4.27",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "es-toolkit": "^1.26.1",
    "file-saver": "^2.0.5",
    "gsap": "^3.12.5",
    "jszip": "^3.10.1",
    "lucide-vue-next": "^0.454.0",
    "nodemailer": "^6.9.16",
    "nuxt": "^3.14.1592",
    "nuxt-auth-utils": "^0.3.9",
    "nuxt-monaco-editor": "^1.3.1",
    "nuxt-posthog": "^1.5.4",
    "nuxt-security": "2.0.0",
    "nuxt-time": "^1.0.2",
    "octokit": "^4.0.2",
    "postgres": "^3.4.5",
    "radix-vue": "^1.9.8",
    "rehype-parse": "^9.0.1",
    "rehype-remark": "^10.0.0",
    "remark-flexible-code-titles": "^1.2.0",
    "remark-gfm": "^4.0.0",
    "shadcn-nuxt": "^0.10.4",
    "strip-markdown": "^6.0.0",
    "tailwind-merge": "^2.5.4",
    "vaul-vue": "^0.2.0",
    "vee-validate": "^4.14.6",
    "vue": "^3.5.11",
    "vue-dndrop": "^1.3.1",
    "vue-router": "^4.4.5",
    "vue-sonner": "^1.2.5",
    "vue3-lottie": "^3.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@iconify-json/radix-icons": "^1.2.1",
    "@iconify/vue": "^4.1.2",
    "@mermaid-js/mermaid-cli": "^11.4.2",
    "@nuxt/eslint": "^0.6.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@tailwindcss/typography": "^0.5.15",
    "@types/file-saver": "^2.0.7",
    "@types/nodemailer": "^6.4.16",
    "@vueuse/core": "^11.1.0",
    "@vueuse/nuxt": "^11.1.0",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.24.2",
    "postcss": "^8.4.47",
    "sass-embedded": "^1.80.4",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.3",
    "vite-plugin-remove-console": "^2.2.0",
    "vue-tsc": "~2.1.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.3"
  },
  "pnpm": {
    "overrides": {
      "drizzle-orm": "^0.33.0"
    }
  }
}
