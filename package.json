{
  "name": "neptun.web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "set EDITOR=code && nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "check": "npx nuxi typecheck",
    "lint": "npx prettier --plugin-search-dir . --check . && npx eslint .",
    "format": "npx prettier --write . --single-quote --trailing-comma es5 --semi",
    "ui": "npx shadcn-vue@latest add",
    "db:push": "drizzle-kit push --config drizzle.config.ts",
    "db:pull": "drizzle-kit introspect --config drizzle.config.ts",
    "db:generate": "drizzle-kit generate --dialect postgresql --schema ./lib/types/database.tables/schema.ts --out=./server/database/migrations",
    "db:migrate": "bun run ./server/utils/migrate.ts",
    "db:studio": "drizzle-kit studio --config drizzle.config.ts --host 127.0.0.1",
    "fresh": "pnpm rebuild && npx nuxi cleanup",
    "types": "npx nuxi prepare",
    "upgrade": "npx nuxi upgrade",
    "info": "npx nuxi info",
    "statistics": "npx nuxi analyze"
  },
  "dependencies": {
    "@ai-sdk/vue": "^0.0.53",
    "@formkit/auto-animate": "^0.8.2",
    "@huggingface/inference": "^2.7.0",
    "@neondatabase/serverless": "^0.9.5",
    "@nuxt/image": "^1.8.0",
    "@nuxt/kit": "^3.13.2",
    "@nuxtjs/mdc": "^0.8.3",
    "@nuxtjs/robots": "^4.1.7",
    "@octokit/plugin-paginate-rest": "^11.3.3",
    "@radix-icons/vue": "^1.0.0",
    "@tanstack/vue-table": "^8.20.5",
    "@vee-validate/zod": "^4.13.2",
    "ai": "^3.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "es-toolkit": "^1.21.0",
    "gsap": "^3.12.5",
    "lucide-vue-next": "^0.446.0",
    "nuxt": "^3.13.2",
    "nuxt-auth-utils": "^0.3.0",
    "nuxt-monaco-editor": "^1.2.9",
    "nuxt-og-image": "^3.0.2",
    "nuxt-posthog": "^1.5.3",
    "nuxt-security": "2.0.0-rc.9",
    "nuxt-time": "^1.0.0",
    "octokit": "^4.0.2",
    "postgres": "^3.4.4",
    "radix-vue": "^1.9.6",
    "rehype-parse": "^9.0.0",
    "rehype-remark": "^10.0.0",
    "remark-flexible-code-titles": "^1.2.0",
    "remark-gfm": "^4.0.0",
    "shadcn-nuxt": "^0.10.4",
    "strip-markdown": "^6.0.0",
    "tailwind-merge": "^2.4.0",
    "undici": "^6.19.8",
    "vaul-vue": "^0.2.0",
    "vee-validate": "^4.13.2",
    "vue": "^3.4.38",
    "vue-dndrop": "^1.3.1",
    "vue-sonner": "^1.2.1",
    "vue3-lottie": "^3.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@iconify-json/radix-icons": "^1.2.0",
    "@iconify/vue": "^4.1.2",
    "@nuxt/eslint": "^0.4.0",
    "@nuxtjs/color-mode": "^3.5.1",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@octokit/types": "^13.5.1",
    "@tailwindcss/typography": "^0.5.15",
